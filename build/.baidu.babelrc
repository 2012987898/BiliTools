{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "12.2"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "comments": false,
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@": "./src",
          "#": "./src/types"
        }
      }
    ]
  ]
}
